{
  "manifest_version": 2,

  "name": "MAL Assist",
  "description": "Automatic assistant for managing anime on MAL",
  "version": "0.1",
  "background": {
    "scripts": [
      "./reducers/index.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*",
    "activeTab"
  ]
}
